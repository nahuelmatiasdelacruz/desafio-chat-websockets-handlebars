{{#if products}}
    <table class="productos">
        <tr>
            <th>Titulo</th>
            <th>Precio</th>
            <th>Imagen</th>
        </tr>
        <tbody>
            {{#each products}}
                <tr>
                    <td>{{this.title}}</td>
                    <td>{{this.price}}</td>
                    <td><img src="{{this.thumbnail}}"/></td>
                </tr>
            {{/each}}
        </tbody>
    </table>
{{else}}
<p>No hay productos</p>
{{/if}}